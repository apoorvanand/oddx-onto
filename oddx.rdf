<?xml version="1.0"?>


<!DOCTYPE rdf:RDF [
    <!ENTITY terms "http://purl.org/dc/terms/" >
    <!ENTITY foaf "http://xmlns.com/foaf/0.1/" >
    <!ENTITY oddx "http://purl.org/openddx/oddx#" >
    <!ENTITY bibo "http://purl.org/ontology/bibo/" >
    <!ENTITY owl "http://www.w3.org/2002/07/owl#" >
    <!ENTITY obo "http://purl.obolibrary.org/obo/" >
    <!ENTITY xsd "http://www.w3.org/2001/XMLSchema#" >
    <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#" >
    <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#" >
]>


<rdf:RDF xmlns="http://openddx.net/onto/oddx#"
     xml:base="http://openddx.net/onto/oddx"
     xmlns:oddx="http://purl.org/openddx/oddx#"
     xmlns:obo="http://purl.obolibrary.org/obo/"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:foaf="http://xmlns.com/foaf/0.1/"
     xmlns:bibo="http://purl.org/ontology/bibo/"
     xmlns:terms="http://purl.org/dc/terms/"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#">
    <owl:Ontology rdf:about="http://purl.org/openddx/oddx">
        <rdfs:label>The OpenDDX ontology</rdfs:label>
        <rdfs:comment>An ontology for connecting diseases with symptoms, via statements in documents or obsetrvations in patients</rdfs:comment>
        <rdfs:comment>Development version at: https://github.com/openddx/oddx-ontology</rdfs:comment>
        <owl:versionInfo>base 0.1, under development</owl:versionInfo>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Datatypes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://purl.org/dc/terms/creator -->

    <owl:ObjectProperty rdf:about="&terms;creator"/>
    


    <!-- http://purl.org/openddx/oddx#disease -->

    <owl:ObjectProperty rdf:about="&oddx;disease">
        <rdfs:domain rdf:resource="&oddx;DiseaseFindingRecord"/>
        <owl:inverseOf rdf:resource="&oddx;hasFindingRecord"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/openddx/oddx#finding -->

    <owl:ObjectProperty rdf:about="&oddx;finding">
        <rdfs:domain rdf:resource="&oddx;DiseaseFindingRecord"/>
        <rdfs:range rdf:resource="&oddx;Finding"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/openddx/oddx#hasDiseaseRecord -->

    <owl:ObjectProperty rdf:about="&oddx;hasDiseaseRecord">
        <rdfs:range rdf:resource="&oddx;DiseaseFindingRecord"/>
        <owl:inverseOf rdf:resource="&oddx;finding"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/openddx/oddx#hasFinding -->

    <owl:ObjectProperty rdf:about="&oddx;hasFinding">
        <rdfs:comment>The simplest association of a disease with a finding (= symptom sensu lato)</rdfs:comment>
        <rdfs:domain rdf:resource="&oddx;Disease"/>
        <rdfs:range rdf:resource="&oddx;Finding"/>
        <owl:inverseOf rdf:resource="&oddx;isFindingOf"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/openddx/oddx#hasFindingRecord -->

    <owl:ObjectProperty rdf:about="&oddx;hasFindingRecord">
        <rdfs:range rdf:resource="&oddx;DiseaseFindingRecord"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/openddx/oddx#inPatient -->

    <owl:ObjectProperty rdf:about="&oddx;inPatient">
        <rdfs:domain rdf:resource="&oddx;DiseaseFindingRecord"/>
        <rdfs:range rdf:resource="&foaf;Person"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/openddx/oddx#isFindingOf -->

    <owl:ObjectProperty rdf:about="&oddx;isFindingOf">
        <rdfs:comment>The simplest association of a disease with a finding (= symptom sensu lato)</rdfs:comment>
        <rdfs:range rdf:resource="&oddx;Disease"/>
        <rdfs:domain rdf:resource="&oddx;Finding"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/openddx/oddx#reference -->

    <owl:ObjectProperty rdf:about="&oddx;reference">
        <rdfs:range rdf:resource="&bibo;Document"/>
        <rdfs:domain rdf:resource="&oddx;DiseaseFindingRecord"/>
    </owl:ObjectProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://purl.org/dc/terms/created -->

    <owl:DatatypeProperty rdf:about="&terms;created"/>
    


    <!-- http://purl.org/openddx/oddx#datePresent -->

    <owl:DatatypeProperty rdf:about="&oddx;datePresent">
        <rdfs:comment>The date the disease and symptom were present in the patient</rdfs:comment>
        <rdfs:range rdf:resource="&xsd;dateTime"/>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/openddx/oddx#estProbOfDisease -->

    <owl:DatatypeProperty rdf:about="&oddx;estProbOfDisease">
        <rdfs:comment>Estimated probability of the number of cases with an observed symptom in which a disease was found</rdfs:comment>
        <rdfs:range rdf:resource="&xsd;decimal"/>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/openddx/oddx#estProbOfFinding -->

    <owl:DatatypeProperty rdf:about="&oddx;estProbOfFinding">
        <rdfs:comment>Estimated probability of the number of cases of a disease in which a symptom was observed</rdfs:comment>
        <rdfs:range rdf:resource="&xsd;decimal"/>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/openddx/oddx#population -->

    <owl:DatatypeProperty rdf:about="&oddx;population">
        <rdfs:comment>Description of the population the record is drawn from. This would optimally point to another ontology for ethnic/geographic populations.</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- http://xmlns.com/foaf/0.1/age -->

    <owl:DatatypeProperty rdf:about="&foaf;age"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://purl.org/ontology/bibo/Document -->

    <owl:Class rdf:about="&bibo;Document"/>
    


    <!-- http://purl.org/openddx/oddx#DiagnosticTest -->

    <owl:Class rdf:about="&oddx;DiagnosticTest">
        <rdfs:label>Diagnostic Test</rdfs:label>
        <rdfs:subClassOf rdf:resource="&oddx;Finding"/>
        <rdfs:comment>A Class for disgnostic tests</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/openddx/oddx#Disease -->

    <owl:Class rdf:about="&oddx;Disease">
        <rdfs:label>Disease</rdfs:label>
        <rdfs:comment>A disease class: eqivalent to obo:DOID_4</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/openddx/oddx#DiseaseFindingRecord -->

    <owl:Class rdf:about="&oddx;DiseaseFindingRecord">
        <rdfs:label>Disease x Finding Record</rdfs:label>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&oddx;hasDiseaseRecord"/>
                <owl:onClass rdf:resource="&oddx;Disease"/>
                <owl:qualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:qualifiedCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&oddx;hasFindingRecord"/>
                <owl:onClass rdf:resource="&oddx;Finding"/>
                <owl:qualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:qualifiedCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>The core record/datum/observation associating a disease with a symptom. Can be either an observation of a single disease in a patient, or an association of a symptom with a disease from a publication.  Should only have a single associated disease, but may have multiple associated symptoms.</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/openddx/oddx#Finding -->

    <owl:Class rdf:about="&oddx;Finding">
        <rdfs:label>Finding</rdfs:label>
        <rdfs:comment>The general class of findings about a disease in a patient, may include lab tests, symptoms (sensu stricto), signs (sensu stricto).</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/openddx/oddx#Sign -->

    <owl:Class rdf:about="&oddx;Sign">
        <rdfs:subClassOf rdf:resource="&oddx;Finding"/>
    </owl:Class>
    


    <!-- http://purl.org/openddx/oddx#Symptom -->

    <owl:Class rdf:about="&oddx;Symptom">
        <rdfs:label>Symptom</rdfs:label>
        <rdfs:subClassOf rdf:resource="&oddx;Finding"/>
        <rdfs:comment>Symptoms (sensu stricto). Equivalent to obo:SYMP_0000462</rdfs:comment>
    </owl:Class>
    


    <!-- http://xmlns.com/foaf/0.1/Person -->

    <owl:Class rdf:about="&foaf;Person"/>
</rdf:RDF>



